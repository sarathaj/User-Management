# Django Project Environment Variables Template
# Copy this file to .env and fill in your actual values.
# This file can be safely committed to version control.

# SECURITY WARNING: keep the secret key used in production secret!
# Generate a new key with: python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'
SECRET_KEY='your-super-secret-key-goes-here'

# SECURITY WARNING: don't run with debug turned on in production!
DEBUG=True

# Comma-separated list of hosts.
ALLOWED_HOSTS=127.0.0.1,localhost

# Database URL (example for SQLite)
# DATABASE_URL=sqlite:///db.sqlite3

# Database URL (example for PostgreSQL)
# DATABASE_URL=postgres://user:password@host:port/dbname